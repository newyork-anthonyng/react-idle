{
  "name": "react-idle",
  "version": "2.0.0",
  "description": "Notifies your app when the user is idle.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "watch -p '{src/**/*.js,README.md,templates/*.html}' -c 'echo \"$(cat templates/header.html) $(render-markdown-js README.md) $(cat templates/footer.html)\" > index.html'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "15.x"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "jest": "^21.2.1",
    "nwb": "0.17.x",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "render-markdown-js": "^1.3.0",
    "watch-cli": "^0.2.2"
  },
  "author": "Ryan Florence",
  "license": "MIT",
  "repository": "https://github.com/reacttraining/react-idle",
  "keywords": [
    "react-component"
  ]
}
